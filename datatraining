import streamlit as st
import pandas as pd

# Load Data Training
@st.cache
def load_training_data():
    datasetTrain = pd.read_csv('padi_produktifitas_training20 (1).csv', delimiter=';', encoding='utf-8')
    datasetTrain.drop(columns=['no', 'kabupaten/kota'], inplace=True)
    return datasetTrain

st.title("Data Training")
st.write("Halaman ini menampilkan data training yang digunakan untuk melatih model.")

datasetTrain = load_training_data()
st.dataframe(datasetTrain)
